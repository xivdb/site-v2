<div class="xivdb-map xiv-cm-embed"></div>
<script>
$(function() {
    // loadmap via placename id
    XIVDBMaps
        .setOption('width', isOnMobile() ? 300 : 820)
        .embed('.xivdb-map', {{ content.placename.id }}, function() {
            XIVDBMaps.Markers.load();
        });
});
</script>